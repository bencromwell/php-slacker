{
    "name": "cromwell/slacker",
    "type": "library",
    "description": "Slack notifier",
    "keywords": ["slack"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Slacker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slacker\\Tests\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^0.9.8",
        "squizlabs/php_codesniffer": "^2.8"
    }
}
